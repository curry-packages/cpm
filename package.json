{
    "name": "cpm",
    "version": "2.2.0",
    "author": "Jonas Oberschweiber <jonas@oberschweiber.com>, Michael Hanus <mh@informatik.uni-kiel.de>",
    "maintainer": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Curry Package Manager: a tool to distribute and install Curry libraries and applications",
    "category": [ "Packaging", "Programming" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "dependencies": {
        "base"          : ">= 1.0.0, < 2.0.0",
        "abstract-curry": ">= 2.0.0",
        "boxes"         : ">= 2.0.0",
        "cass-analysis" : ">= 2.0.0",
        "cass"          : ">= 2.0.0",
        "cdbi"          : ">= 2.0.0",
        "csv"           : ">= 1.0.0",
        "currypath"     : ">= 0.0.1",
        "det-parse"     : ">= 0.0.1",
        "execpath"      : ">= 0.0.1",
        "flatcurry"     : ">= 2.0.0",
        "frontend-exec" : ">= 0.0.1",
        "json"          : ">= 2.0.0",
        "opt-parse"     : ">= 0.0.3",
        "profiling"     : ">= 1.0.0",
        "propertyfile"  : ">= 0.0.1",
        "redblacktree"  : ">= 0.0.1",
        "wl-pprint"     : ">= 0.0.1"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.3",
        "kics2": ">= 2.0.3"
    },
    "configModule": "CPM.ConfigPackage",
    "executable": {
        "name": "cypm",
        "main": "CPM.Main",
        "options": { "kics2" : ":set rts -T" }
    },
    "testsuite": {
        "src-dir": "src",
        "modules": [ "CPM.Package", "CPM.Resolution", "CPM.LookupSet" ]
    },
    "documentation": {
        "src-dir": "docs",
        "main": "manual.tex"
    },
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/cpm.git",
        "tag": "$version"
    },
    "homepage": "http://www.curry-lang.org/tools/cpm",
    "repository": "https://git.ps.informatik.uni-kiel.de/curry-packages/cpm.git"
}
